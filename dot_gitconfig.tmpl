[user]
	name = {{ .name }}
	email = {{ .git.email }}
	signingKey = {{ .git.signingKey }}
[credential]
	helper = {{ .git.credential.helper }}
[pull]
	rebase = true
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[rebase]
	autosquash = true
	autostash = true
[github]
	user = {{ .github.user }}
[commit]
	gpgSign = true
[credential "https://dev.azure.com"]
	useHttpPath = true
